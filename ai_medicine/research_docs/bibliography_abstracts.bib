  author = {Manuel Corpas and Vicente Soriano and Teresa Perucho and Juan S Rincón-Redondo and Manuel Pérez-Alonso and Juan A G Ranea and Carmen de Mendoza and Federico Morán},
  title = {Playing catching up: Proceedings of the 1},
