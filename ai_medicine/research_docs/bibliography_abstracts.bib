  author = {Karlo Petkovic and Zdeslav Strika and Robert Likic and Marko Lucijanic},
  title = {Spotlight commentary: Integrating artificial intelligence in clinical pharmacology: Opportunities, challenges and ethical imperatives.},
  journal = {British journal of clinical pharmacology},
